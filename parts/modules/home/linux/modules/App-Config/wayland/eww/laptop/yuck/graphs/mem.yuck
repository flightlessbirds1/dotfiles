(defwidget mem []
  (box
    :class "graphBox"
    :orientation "vertical"
    :tooltip "${round(EWW_RAM.used_mem_perc, 1)}%"
    :width 60
    (graph
      :class "memGraph"
      :value {EWW_RAM.used_mem_perc}
      :time-range "30s"
    )
    (label
      :class "memText"
      :text "mem"
    )
  )
)
